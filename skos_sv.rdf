  <!--
    Description of this version of the annotations.
  -->
  <rdf:Description rdf:about="http://www.w3.org/2004/02/skos/core/annotations/sv/2013-07-14.rdf">
    <rdfs:label xml:lang="en">Swedish annotations for the SKOS Core Vocabulary, 2013-07-14 version</rdfs:label>
    <dc:date>2013-07-14</dc:date>
    <dct:isVersionOf rdf:resource="http://www.w3.org/2004/02/skos/core_sv"/>
